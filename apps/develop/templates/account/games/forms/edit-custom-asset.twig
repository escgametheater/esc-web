<div class="column is-fullwidth">
    {{ form.slug()|raw }}
</div>
<div class="column is-fullwidth">
    {{ form.is_public|raw }}
</div>
<button type="submit" class="button is-primary" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="edit-custom-asset" data-esc-tracking-element="submit">{% t "Save" %}</button>
<a href="{{ game.edit_url }}/{{ update_channel }}/delete-custom-game-asset/{{ custom_game_asset_link.slug }}/?game_build_id={{ game_build.getPk() }}" data-esc-fancybox="default" class="button is-danger" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="edit-custom-asset" data-esc-tracking-element="delete">Delete Entire Asset</a>