<div class="flex">
    <div class="column is-12">

        <h1 class="title is-4 is-fullwidth">

            {% set active_build_tab = get.active_build_tab %}

            <a href="{{ develop_url }}/teams/{{ active_organization.slug }}/" class="esc-link" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="team-dashboard">
                <span class="image is-creator is-tiny is-inline-block" style="top: 2px;"><img src="{{ active_organization.avatar.small_url }}" /></span> <a class="esc-link ml-xs" href="{{ develop_url }}/teams/{{ active_organization.slug }}/">{{ active_organization.display_name }}</a>
            </a>

            {#{% if count(organizations) > 1 %}#}
                {#{% include "account/blocks/org-selector-form.twig" %}#}
            {#{% else %}#}
                {#<a href="{{ develop_url }}/teams/{{ active_organization.slug }}/" class="esc-link">#}
                    {#<span class="image is-creator is-tiny is-inline-block" style="top: 2px;"><img src="{{ active_organization.avatar.small_url }}" /></span> <a class="esc-link ml-xs" href="{{ develop_url }}/teams/{{ active_organization.slug }}/">{{ active_organization.display_name }}</a>#}
                {#</a>#}
            {#{% endif %}#}

            <div class="is-inline-block pt-xs" style="padding-top:6px !important;">
                &raquo;

            <a class="esc-link" href="/teams/{{ active_organization.slug }}/manage-games" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="manage-games">Games</a> &raquo;
            {% if page == 'game-settings' %}
                {{ game.display_name }}
            {% else %}
                <a class="esc-link" href="{{ game.edit_url }}" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="edit-game" data-esc-tracking-element="{{ game.slug }}">{{ game.display_name }}</a>
            {% endif %}
                
            {% if page == 'game-access-control' %}
                &raquo; User Licenses &raquo; {{ active_update_channel.display_name }}

            {% elseif page == 'game-vtt-interface' %}
                &raquo; VTT Interface &raquo; {{ active_update_channel.display_name }}


            {% elseif page == 'game-builds' %}
                &raquo; Builds &raquo; {{ active_update_channel.display_name }}
            {% elseif page == 'view-game-build' %}
                &raquo; <a href="{{ game.edit_url }}/{{ update_channel }}/builds" class="esc-link" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="manage-builds" data-esc-tracking-element="{{ update_channel }}">Builds</a> &raquo; {{ active_update_channel.display_name }} v{{ game_build.game_build_version }}
            {% elseif page == 'game-view-custom-data' %}
                &raquo; <a href="{{ game.edit_url }}/{{ update_channel }}/builds" class="esc-link" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="manage-builds" data-esc-tracking-element="{{ update_channel }}">Builds</a> &raquo; <a class="esc-link" href="{{ game.edit_url }}/{{ update_channel }}/view-game-build/{{ game_build.getPk() }}" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="edit-game-build" data-esc-tracking-element="{{ update_channel }}">{{ active_update_channel.display_name }} v{{ game_build.game_build_version }}</a> &raquo; <a href="{{ game.edit_url }}/{{ update_channel }}/view-game-build/{{ game_build.getPk() }}?active_build_tab=custom-data" class="esc-link" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="edit-build" data-esc-tracking-element="custom-data">Custom Data</a> &raquo; {{ game_data.key }}
            {% elseif page == 'batch-upload-custom-assets' %}
                &raquo; <a href="{{ game.edit_url }}/{{ update_channel }}/builds" class="esc-link" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="manage-builds" data-esc-tracking-element="{{ update_channel }}">Builds</a> &raquo; <a class="esc-link" href="{{ game.edit_url }}/{{ update_channel }}/view-game-build/{{ game_build.getPk() }}" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="edit-game-build" data-esc-tracking-element="{{ update_channel }}">{{ active_update_channel.display_name }} v{{ game_build.game_build_version }}</a> &raquo; <a href="{{ game.edit_url }}/{{ update_channel }}/view-game-build/{{ game_build.getPk() }}?active_build_tab=custom-assets" class="esc-link" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="edit-build" data-esc-tracking-element="custom-assets">Custom Assets</a> &raquo; Batch Upload
            {% elseif page == 'view-custom-game-asset' %}
                &raquo; <a href="{{ game.edit_url }}/{{ update_channel }}/builds" class="esc-link" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="manage-builds" data-esc-tracking-element="{{ update_channel }}">Builds</a> &raquo; <a class="esc-link" href="{{ game.edit_url }}/{{ update_channel }}/view-game-build/{{ game_build.getPk() }}" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="edit-game-build" data-esc-tracking-element="{{ update_channel }}">{{ active_update_channel.display_name }} v{{ game_build.game_build_version }}</a> &raquo; <a href="{{ game.edit_url }}/{{ update_channel }}/view-game-build/{{ game_build.getPk() }}?active_build_tab=custom-assets" class="esc-link" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="edit-build" data-esc-tracking-element="custom-assets">Custom Assets</a> &raquo; {{ custom_game_asset_link.slug }}
            {% elseif page == 'view-game-controller' %}
                &raquo; <a href="{{ game.edit_url }}/{{ update_channel }}/builds" class="esc-link" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="manage-builds" data-esc-tracking-element="{{ update_channel }}">Builds</a> &raquo; <a class="esc-link" href="{{ game.edit_url }}/{{ update_channel }}/view-game-build/{{ game_build.getPk() }}?active_build_tab=controllers" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="breadcrumb" data-esc-tracking-context="edit-game-build" data-esc-tracking-element="{{ update_channel }}">{{ active_update_channel.display_name }} v{{ game_build.game_build_version }}</a> &raquo; {{ game_controller.display_name }} Controller v{{ game_controller.game_controller_version }}
            {% endif %}
            </div>
        </h1>
    </div>
</div>
