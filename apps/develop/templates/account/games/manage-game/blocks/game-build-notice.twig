{% if update_channel == 'live' %}
    <div class="pb-s sheet is-info pl-l pt-m">
        <h2 class="title is-3 pb-s">Note: All build changes are disabled on {{ active_update_channel.display_name }}. <a href="{{ game.edit_url }}/dev/builds" class="button is-primary is-inline is-small pt-xs pb-s float-right mr-l" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="build-upload-notice" data-esc-tracking-element="change-channel">Switch to Development Channel</a></h2>
    </div>
{% else %}

    {% if game_build.is_published() %}
        <div class="sheet is-info pb-s pl-l pt-m">
            <h2 class="title is-3 pb-s">You can't modify game builds that have already been published. <a href="{{ game.edit_url }}/dev/builds" class="button is-primary is-inline is-small pt-xs pb-s float-right mr-l" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="build-upload-notice" data-esc-tracking-element="upload-game-build">Upload new development build</a></h2>
        </div>
    {% else %}

    {% endif %}


    {#{% if game_build.game_build_id != active_build_version_summary.game_build_id and game_build.game_assets and game_build.game_controllers %}#}
        {#<a href="{{ game.edit_url }}/{{ update_channel }}/set-active-build/{{ game_build.game_build_id }}" class="button is-primary mb-m">Set Build Active</a>#}
    {#{% endif %}#}

{% endif %}

