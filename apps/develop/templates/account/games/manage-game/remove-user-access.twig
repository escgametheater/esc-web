<div class="ajax-modal-container align-center p-l">
    <h3 class="title is-3 mb-l">{% t "Are you sure you want to delete this user license:" %} {{ email }}</h3>
    <p>Start: {{ license.local_start_time|long_date_format }}<br/>End: {% if license.local_end_time %}{{ license.local_end_time|long_date_format }}{% else %}Never{% endif %}</p>
    <p><strong>This can not be un-done!</strong></p>
    <br/>
    <form method="post" action="{{ path }}"  data-esc-ajax-form="json">
        {{ form|raw }}
        <button type="submit" class="button is-primary" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="modal-delete-game-user-license" data-esc-tracking-element="submit">{% t "Yes" %}</button>
        <a href="javascript:parent.$.fancybox.close();" class="button is-white ml-s mr-s" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="modal-delete-game-user-license" data-esc-tracking-element="cancel">{% t "Cancel" %}</a>
    </form>
</div>