{% if update_channel == 'live' %}
    <div class="pb-s sheet is-info pl-l pt-m">
        <h2 class="title is-3 pb-s">Note: All build changes are disabled on {{ active_update_channel.display_name }}. <a href="{{ game_mod.edit_url }}/dev/builds" class="button is-primary is-inline is-small pt-xs pb-s float-right mr-l" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="mod-build-notice" data-esc-tracking-element="swap-to-dev-channel">Switch to Development Channel</a></h2>
    </div>
{% else %}

    {% if game_mod_build.is_published() %}
        <div class="sheet is-info pb-s pl-l pt-m">
            <h2 class="title is-3 pb-s">You can't modify mod builds that have already been published. <a href="{{ game_mod.edit_url }}/dev/builds" class="button is-primary is-inline is-small pt-xs pb-s float-right mr-l" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="mod-build-notice" data-esc-tracking-element="upload-dev-game-mod-build">Upload new development build</a></h2>
        </div>
    {% else %}

    {% endif %}

{% endif %}

