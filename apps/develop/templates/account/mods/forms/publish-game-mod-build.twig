<div class="flex is-wrapping">
    {{ form.game_mod_build_id()|raw }}
    <div class="column is-12 pb-0 pt-0">
        {{ form.is_active()|raw }}
    </div>
    {{ form.next()|raw }}
    <div class="column is-12 pb-0">
        <button type="submit" class="button is-primary" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="modal-publish-game-mod-build" data-esc-tracking-element="submit">Publish Build</button>
        <a href="javascript:parent.$.fancybox.close();" class="button is-white ml-s mr-s" data-esc-tracking-service="mixpanel" data-esc-tracking="clicked" data-esc-tracking-content-type="cta-button" data-esc-tracking-context="modal-publish-game-mod-build" data-esc-tracking-element="cancel">{% t "Cancel" %}</a>
    </div>
</div>
